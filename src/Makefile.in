cpio.o: cpio.c
	$(COMPILE) -DLOCALEDIR=\"$(localedir)\" -c $(srcdir)/cpio.c

cpio._o: cpio._c
	@rm -f _cpio.c
	@ln cpio._c _cpio.c
	$(COMPILE) -DLOCALEDIR=\"$(localedir)\" -c _cpio.c
	@mv _cpio.o $@
	@rm _cpio.c

