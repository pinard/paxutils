  for ac_dir in $PATH; do
    test -z "$ac_dir" && ac_dir=.
    if test $ac_x $ac_dir/$ac_word; then
      ac_cv_path_TEST_GNUTAR="$ac_dir/$ac_word"
      break
    fi
  done
  IFS="$ac_save_ifs"
  ;;
esac
fi
TEST_GNUTAR="$ac_cv_path_TEST_GNUTAR"
if test -n "$TEST_GNUTAR"; then
  echo "$ac_t""$TEST_GNUTAR" 1>&6
else
  echo "$ac_t""no" 1>&6
fi

test -n "$TEST_GNUTAR" && break
done

# Ensure we really have GNU tar.
if test -z "$TEST_GNUTAR" \
   || test -z "`$TEST_GNUTAR --version 2> /dev/null`"; then
   # No.
   TEST_GNUTAR=
fi

# Maybe systems exist without 'id'.  How does one get uid and gid
# information on such machines?
# Extract the first word of "id", so it can be a program name with args.
set dummy id; ac_word=$2
echo $ac_n "checking for $ac_word""... $ac_c" 1>&6
echo "configure:4823: checking for $ac_word" >&5
if eval "test \"`echo '$''{'ac_cv_path_TEST_ID'+set}'`\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  case "$TEST_ID" in
  /*|[A-z]:/*)
  ac_cv_path_TEST_ID="$TEST_ID" # Let the user override the test with a path.
  ;;
  *)
  IFS="${IFS= 	}"; ac_save_ifs="$IFS"; IFS="${IFS}:"
  for ac_dir in $PATH; do
    test -z "$ac_dir" && ac_dir=.
    if test $ac_x $ac_dir/$ac_word; then
      ac_cv_path_TEST_ID="$ac_dir/$ac_word"
      break
    fi
  done
  IFS="$ac_save_ifs"
  ;;
esac
fi
TEST_ID="$ac_cv_path_TEST_ID"
if test -n "$TEST_ID"; then
  echo "$ac_t""$TEST_ID" 1>&6
else
  echo "$ac_t""no" 1>&6
fi


if test -n "$TEST_ID"; then
   # Have it.  Get info.
   idinfo=`$TEST_ID`
   TEST_UID=`echo $idinfo | sed 's/^.*uid=\([0-9][0-9]*\).*$/\1/'`
   TEST_UIDNAME=`echo $idinfo | sed 's/uid=[^(]*(\([^)]*\).*$/\1/'`
   TEST_GID=`echo $idinfo | sed 's/^.*gid=\([0-9][0-9]*\).*$/\1/'`
   TEST_GIDNAME=`echo $idinfo | sed 's/^.*gid=[^(]*(\([^)]*\).*$/\1/'`

   groups=`echo $idinfo | sed 's/^.*groups=\(.*\)$/\1/' | tr , ' '`
   TEST_GID2=$TEST_GID
   TEST_GID2NAME=$TEST_GIDNAME
   for elt in $groups; do
      if test "$elt" != "$TEST_GID($TEST_GIDNAME)"; then
	 TEST_GID2=`echo $elt | sed 's/(.*$//'`
	 TEST_GID2NAME=`echo $elt | sed 's/^.*(\([^)]*\))/\1/'`
	 break
      fi
   done
fi








